<launch>
  <node pkg="hector_turtlebot_scan_filter" type="hector_turtlebot_scan_filter" name="base_laser_scan_filter" output="screen">
    <rosparam file="$(find gki_turtlebot_bringup)/config/scan_filter.yaml" command="load"/>
    <remap from="hokuyo_scan" to="base_scan" />
    <remap from="hokuyo_scan_filtered" to="base_scan_filtered" />
  </node>
</launch>

